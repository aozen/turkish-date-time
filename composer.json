{
    "name": "aozen/turkish-date-time",
    "description": "Turkish Datetimepicker for Laravel Nova",
    "keywords": [
        "laravel",
        "nova",
        "turkish",
        "datetime"
    ],
    "authors": [
        {
            "name": "Ali Ã–zen",
            "email": "aliozendev@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "^5.6",
        "laravel/nova": "*"
    },
    "autoload": {
        "psr-4": {
            "Aozen\\TurkishDateTime\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aozen\\TurkishDateTime\\FieldServiceProvider"
            ]
        }
    },
    "config": {
